{
    "name": "leonardl00/metabase",
    "description": "Embed Metabase dashboard and question in your application",
    "keywords": [
        "leonardl00",
        "laravel",
        "metabase"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Leonard Louis",
            "email": "leo@leonardlouis.dev"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "lcobucci/jwt": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Metabase\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravolt\\Metabase\\MetabaseServiceProvider"
            ]
        },
        "minimum-stability": "dev",
        "prefer-stable": true
    }
}
